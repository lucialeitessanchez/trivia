            </tr>
            <tr>
                <th>Municipio</th>
                <td>{{ respuestum.municipio }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_respuesta_index') }}">back to list</a>

    <a href="{{ path('app_respuesta_edit', {'idrespuesta': respuestum.idrespuesta}) }}">edit</a>

