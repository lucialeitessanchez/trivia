{% extends 'base.html.twig' %}

{% block title %}Respuesta{% endblock %}

{% block body %}
    <h1>Respuesta</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idrespuesta</th>
                <td>{{ respuestum.idrespuesta }}</td>
            </tr>
            <tr>
                <th>Parametromedicion</th>
                <td>{{ respuestum.parametromedicion }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ respuestum.color }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
